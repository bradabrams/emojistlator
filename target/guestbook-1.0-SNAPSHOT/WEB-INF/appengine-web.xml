<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>debuggerdemo</application>
    <version>42</version>
    <threadsafe>true</threadsafe>
    

    <module>default</module>
    <vm>true</vm>
    <manual-scaling>
        <instances>2</instances>
    </manual-scaling>

   <env-variables>
      <!-- this is need for the cloud debubger  -->      
      <env-var name="CDBG_ENABLE" value="1" />
   </env-variables>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>

    <!-- TODO(athicha): Try again later -->
    <!--vm-settings>
      <setting name="apt_get_install" value="redis-server vim"/>
    </vm-settings-->
</appengine-web-app>
