<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>translate</servlet-name>
        <servlet-class>com.google.appengine.demos.guestbook.TranslateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteall</servlet-name>
        <servlet-class>com.google.appengine.demos.guestbook.DeleteAllEntriesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>com.google.appengine.demos.guestbook.GuestbookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>translate</servlet-name>
        <url-pattern>/translate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteall</servlet-name>
        <url-pattern>/deleteall</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>guestbook.jsp</welcome-file>
    </welcome-file-list>
</web-app>
